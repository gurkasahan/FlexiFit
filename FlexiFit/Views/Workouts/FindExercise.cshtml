@model IEnumerable<string>

@{
    ViewData["Title"] = "Find Exercise";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Muscle Groups</h1>

<ul>
    @foreach (var group in Model)
    {
        <li>
            <a asp-action="WorkoutDetails" asp-route-muscleGroup="@group">@group</a>
        </li>
    }
</ul>
